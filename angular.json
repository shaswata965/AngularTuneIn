{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "26f9e84f-b1c7-4d90-84bc-e45949c2920d"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "TuneIn": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/TuneIn",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/metismenujs/dist/metismenujs.css",
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "src/assets/frontend/css/animate.css",
              "src/assets/frontend/css/bootstrap.min.css",
              "src/assets/frontend/css/fonts.css",
              "src/assets/frontend/css/flaticon.css",
              "src/assets/frontend/css/font-awesome.css",
              "src/assets/frontend/css/owl.carousel.css",
              "src/assets/frontend/css/owl.theme.default.css",
              "src/assets/frontend/css/nice-select.css",
              "src/assets/frontend/css/swiper.css",
              "src/assets/frontend/css/magnific-popup.css",
              "src/assets/frontend/css/style.css",
              "src/assets/frontend/css/responsive.css",
              "src/assets/frontend/css/custom.css",
              "src/assets/frontend/css/custom_responsive.css",
              "src/assets/frontend/css/space.css",
              "src/styles.css",
              "src/assets/backend/vendors/themefy_icon/themify-icons.css",
              "src/assets/backend/vendors/niceselect/css/nice-select.css",
              "src/assets/backend/vendors/gijgo/gijgo.min.css",
              "src/assets/backend/vendors/font_awesome/css/all.min.css",
              "src/assets/backend/vendors/tagsinput/tagsinput.css",
              "src/assets/backend/vendors/datepicker/date-picker.css",
              "src/assets/backend/vendors/vectormap-home/vectormap-2.0.2.css",
              "src/assets/backend/vendors/scroll/scrollable.css",
              "src/assets/backend/vendors/datatable/css/jquery.dataTables.min.css",
              "src/assets/backend/vendors/datatable/css/responsive.dataTables.min.css",
              "src/assets/backend/vendors/datatable/css/buttons.dataTables.min.css",
              "src/assets/backend/vendors/text_editor/summernote-bs4.css",
              "src/assets/backend/vendors/morris/morris.css",
              "src/assets/backend/vendors/material_icon/material-icons.css",
              "src/assets/backend/css/metisMenu.css",
              "src/assets/backend/css/style.css",
              "src/assets/backend/css/colors/default.css",
              "src/assets/backend/css/space.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "src/assets/frontend/js/modernizr.js",
              "src/assets/frontend/js/plugin.js",
              "src/assets/frontend/js/jquery.nice-select.min.js",
              "src/assets/frontend/js/jquery.inview.min.js",
              "src/assets/frontend/js/jquery.magnific-popup.js",
              "src/assets/frontend/js/swiper.min.js",
              "src/assets/frontend/js/comboTreePlugin.js",
              "src/assets/frontend/js/owl.carousel.js",
              "src/assets/frontend/js/custom.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "src/assets/backend/js/popper.min.js",
              "src/assets/backend/js/bootstrap.min.js",
              "src/assets/backend/vendors/count_up/jquery.waypoints.min.js",
              "src/assets/backend/vendors/chartlist/Chart.min.js",
              "src/assets/backend/vendors/count_up/jquery.counterup.min.js",
              "src/assets/backend/vendors/datatable/js/jquery.dataTables.min.js",
              "src/assets/backend/vendors/datatable/js/dataTables.responsive.min.js",
              "src/assets/backend/vendors/datatable/js/dataTables.buttons.min.js",
              "src/assets/backend/vendors/datatable/js/buttons.flash.min.js",
              "src/assets/backend/vendors/datatable/js/jszip.min.js",
              "src/assets/backend/vendors/datatable/js/pdfmake.min.js",
              "src/assets/backend/vendors/datatable/js/vfs_fonts.js",
              "src/assets/backend/vendors/datatable/js/buttons.html5.min.js",
              "src/assets/backend/vendors/datatable/js/buttons.print.min.js",
              "src/assets/backend/vendors/datepicker/datepicker.js",
              "src/assets/backend/vendors/datepicker/datepicker.en.js",
              "src/assets/backend/vendors/datepicker/datepicker.custom.js",
              "src/assets/backend/js/chart.min.js",
              "src/assets/backend/vendors/chartjs/roundedBar.min.js",
              "src/assets/backend/vendors/progressbar/jquery.barfiller.js",
              "src/assets/backend/vendors/tagsinput/tagsinput.js",
              "src/assets/backend/vendors/text_editor/summernote-bs4.js",
              "src/assets/backend/vendors/am_chart/amcharts.js",
              "src/assets/backend/vendors/scroll/perfect-scrollbar.min.js",
              "src/assets/backend/vendors/scroll/scrollable-custom.js",
              "src/assets/backend/vendors/vectormap-home/vectormap-2.0.2.min.js",
              "src/assets/backend/vendors/vectormap-home/vectormap-world-mill-en.js",
              "src/assets/backend/vendors/apex_chart/apex-chart2.js",
              "src/assets/backend/vendors/apex_chart/apex_dashboard.js",
              "src/assets/backend/vendors/echart/echarts.min.js",
              "src/assets/backend/vendors/chart_am/core.js",
              "src/assets/backend/vendors/chart_am/charts.js",
              "src/assets/backend/vendors/chart_am/animated.js",
              "src/assets/backend/vendors/chart_am/kelly.js",
              "src/assets/backend/vendors/chart_am/chart-custom.js",
              "src/assets/backend/js/dashboard_init.js",
              "src/assets/backend/js/custom.js",
              "node_modules/chart.js/dist/Chart.js",
              "src/assets/backend/js/metisMenu.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "TuneIn:build:production"
            },
            "development": {
              "browserTarget": "TuneIn:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "TuneIn:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "TuneIn"
}
